a=int(input("enter  num 1"))
b=int(input("enter num 2"))
c=int(input("enter num 3"))
if(a>b)and(a>c):
    print("a is greatest")
elif(b>a) and(b>c):
    print("b is greatest")
elif(c>a) and(c>b):
    print("c is greatest")
elif(a==b) and (c>a):
      print("c is greatest")
else:
    print("default")
